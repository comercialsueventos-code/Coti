{
  "name": "sue-events-quotes",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "vite build",
    "build:prod:safe": "tsc --project tsconfig.prod.json && vite build",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "duplicate-check": "jscpd src/ --min-lines 3 --min-tokens 50 --format \"typescript,tsx\" --reporters \"console\"",
    "duplicate-report": "jscpd src/ --min-lines 3 --min-tokens 50 --format \"typescript,tsx\" --reporters \"console,html\" --output \"./docs/duplicate-report\"",
    "duplicate-quick": "jscpd src/ --min-lines 5 --min-tokens 100 --format \"typescript,tsx\" --reporters \"console\"",
    "quality-check": "npm run lint && npm run duplicate-check",
    "refactor-analyze": "npm run duplicate-report && echo '\\nðŸ“Š Reporte HTML disponible en: docs/duplicate-report/html/'",
    "consolidation:start": "node scripts/consolidation-start.js",
    "consolidation:validate": "node scripts/consolidation-validate.js",
    "consolidation:rollback": "node scripts/consolidation-rollback.js",
    "test:unit": "jest --config tests/jest.config.consolidation.js --testPathPattern=unit",
    "test:integration": "node scripts/run-integration-tests.js",
    "test:snapshot": "node scripts/run-snapshot-tests.js",
    "test:performance": "node scripts/run-performance-tests.js",
    "test:consolidation": "node scripts/test-consolidation.js",
    "test:e2e": "echo 'E2E tests not configured yet'",
    "test:coverage": "jest --config tests/jest.config.consolidation.js --coverage",
    "test:smoke": "npm run lint && npm run type-check && npm run build",
    "test:full": "npm run test:unit && npm run test:integration && npm run lint && npm run type-check",
    "build:analyze": "npm run build && echo '{\"bundleSize\": \"analyzed\", \"timestamp\": \"'$(date -Iseconds)'\"}'",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/x-date-pickers": "^6.18.0",
    "@supabase/supabase-js": "^2.56.1",
    "@tanstack/react-query": "^5.17.0",
    "date-fns": "^2.30.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-router-dom": "^6.20.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-big-calendar": "^1.8.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jscpd": "^4.0.5",
    "chalk": "^4.1.2",
    "puppeteer": "^24.18.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-html-reporters": "^3.1.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.0",
    "babel-jest": "^29.7.0",
    "@types/jest": "^29.5.0"
  }
}
